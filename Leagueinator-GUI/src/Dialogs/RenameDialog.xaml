<Window x:Class="Leagueinator.GUI.Dialogs.RenameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Leagueinator.GUI.Dialogs"
        mc:Ignorable="d"
        Title="RenameDialog" Height="150" Width="300">
    <StackPanel Margin="10">
        <Label Content="New Name:"/>
        <TextBox x:Name="txtNewName" Margin="0,5,0,0"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,10,0,0">
            <Button x:Name="ButOk" Content="OK" Width="80" Margin="0,0,5,0" Click="HndOkButtonClick"/>
            <Button Content="Cancel" Width="80" Click="HndCancelButtonClick"/>
        </StackPanel>
    </StackPanel>
</Window>